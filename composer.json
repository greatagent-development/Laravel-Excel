{
  "name": "maatwebsite/excel",
  "description": "Supercharged Excel exports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/greatagent-development/PHPExcel"
    }
  ],
  "require": {
    "php": "^8.0",
    "phpoffice/phpexcel": "dev-php81",
    "illuminate/support": "^9.0",
    "jeremeamia/superclosure": "^2.3",
    "tijsverkoyen/css-to-inline-styles": "~2.0",
    "laravel/helpers": "^1.4"
  },
  "require-dev": {
    "phpseclib/phpseclib": "~1.0",
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^7.6"
  },
  "suggest": {
    "illuminate/http": "^9.0",
    "illuminate/routing": "^9.0",
    "illuminate/view": "^9.0",
    "illuminate/queue": "^9.0"
  },
  "autoload": {
    "classmap": [
      "src/Maatwebsite/Excel"
    ],
    "psr-0": {
      "Maatwebsite\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
        "laravel": {
            "providers": [
                "Maatwebsite\\Excel\\ExcelServiceProvider"
            ],
            "aliases": {
                "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
            }
        }
    }
}
